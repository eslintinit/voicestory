{
  "name": "voicestory-backend",
  "scripts": {
    "postinstall": "npm -s run generate",
    "seed": "ts-node prisma/seed.ts",
    "dev": "NODE_ENV=development ts-node-dev --no-notify --respawn --transpileOnly ./src",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "NODE_ENV=production prisma-multi-tenant generate",
    "generate:nexus": "NODE_ENV=production ts-node --transpile-only src/index.ts",
    "new_service": "node scripts/prisma_new_service/index.js",
    "delete_service": "prisma-multi-tenant delete",
    "deploy": "node scripts/prisma_deploy_all/index.js"
  },
  "dependencies": {
    "@nexus/schema": "^0.13.1",
    "@prisma/client": "^2.0.0-beta.1",
    "aws-sdk": "2.651.0",
    "bcryptjs": "2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "2.8.5",
    "dotenv": "^8.2.0",
    "global": "^4.4.0",
    "graphql": "^14.5.8",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.0.14",
    "graphql-upload": "8.1.0",
    "graphql-yoga": "1.18.3",
    "jsonwebtoken": "8.5.1",
    "mkdirp": "1.0.3",
    "nexus": "^0.12.0",
    "nexus-prisma": "^0.12.0",
    "open-graph-scraper": "^3.6.2",
    "passport": "^0.4.1",
    "passport-twitter-token": "^1.3.0",
    "prisma": "^1.34.10",
    "prisma-multi-tenant": "^2.0.0-beta.1",
    "request": "^2.88.2",
    "shortid": "2.2.15",
    "subscriptions-transport-ws": "0.9.16",
    "twitter": "^1.7.1",
    "uuid": "7.0.3"
  },
  "devDependencies": {
    "@prisma/cli": "^2.0.0-alpha.1031",
    "@types/bcryptjs": "2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.4",
    "@types/graphql-upload": "8.0.3",
    "@types/jsonwebtoken": "8.3.8",
    "@types/mkdirp": "1.0.0",
    "@types/node": "13.11.0",
    "@types/passport": "^1.0.3",
    "@types/request": "^2.48.4",
    "@types/twitter": "^1.7.0",
    "prettier": "^2.0.2",
    "ts-node": "8.8.1",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.8.3"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
